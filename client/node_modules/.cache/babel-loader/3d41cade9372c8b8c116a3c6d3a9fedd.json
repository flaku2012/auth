   </div>\r\n\r\n                <div class=\"mb-3\">\r\n                    <label class=\"form-label\">Powtórz hasło:</label>\r\n                    <input type=\"password\" class=\"form-control\" v-model=\"form.repeat_password\">\r\n                    <div class=\"form-text text-danger\" v-if=\"errors.repeat_password\">{{errors.repeat_password[0]}}</div>\r\n                </div>\r\n\r\n                <div class=\"col-md-6 offset-md-4\">\r\n                    <button type=\"submit\" class=\"btn btn-primary\">\r\n                        Zarejestruj się\r\n                    </button>\r\n                </div>\r\n            </div>\r\n            </form>\r\n        </div>\r\n    </div>\r\n    <div class=\"col-3\">\r\n        TESTY: <br>\r\n        {{errors}}\r\n    </div>\r\n</div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'register',\r\n  components: {\r\n    //\r\n  },\r\n\r\n  data(){\r\n      return {\r\n          errors: [],\r\n          form:{\r\n              name: '',\r\n              email: '',\r\n              password: '',\r\n              repeat_password: '',\r\n            },\r\n      }\r\n  },\r\n\r\n  methods:{\r\n        async submit(){\r\n            await axios.post('auth/register', {\r\n            name: this.form.name,\r\n            email: this.form.email,\r\n            password: this.form.password,\r\n            repeat_password: this.form.repeat_password,\r\n        })\r\n        .catch( (error) => {\r\n            // handle error\r\n            this.errors = error.response.data.errors\r\n            console.log(error.response.data)\r\n        })\r\n        .finally(()=>{\r\n            this.$router.replace('/signin')\r\n            this.form.name = ''\r\n            this.form.email = ''\r\n            this.form.password = ''\r\n            this.form.repeat_password = ''\r\n        })\r\n      }\r\n  }\r\n\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}